{
  "manifest_version": 3,
  "name": "Reflect",
  "version": "1.0.0",
  "description": "A calm moment of reflection with every new tab",
  "permissions": ["storage", "search"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
